CREATE TABLE `singular_app`.`PRIVILEGIO`(
  `ID_PRIVILEGIO` INT NOT NULL AUTO_INCREMENT,
  `CODIGO_PRIVILEGIO` VARCHAR(50) NOT NULL,
  PRIMARY KEY (`ID_PRIVILEGIO`)
) ENGINE = MyISAM;

CREATE TABLE `singular_app`.`PERFIL`(
  `ID_PERFIL` INT NOT NULL AUTO_INCREMENT,
  `NOMBRE_PERFIL` VARCHAR(30) NULL,
  PRIMARY KEY (`ID_PERFIL`)
) ENGINE = MyISAM;

CREATE TABLE `singular_app`.`PERFIL_PRIVILEGIO`(
  `ID_PRIVILEGIO` INT NOT NULL AUTO_INCREMENT,
  `ID_PERFIL` INT NOT NULL,
  PRIMARY KEY (`ID_PRIVILEGIO`,`ID_PERFIL`)
) ENGINE = MyISAM;

CREATE TABLE `singular_app`.`USUARIO` (
  `ID_USUARIO` INT NOT NULL AUTO_INCREMENT,
  `NOMBRE` VARCHAR(256) NOT NULL,
  `APELLIDO` VARCHAR(100) NOT NULL,
  `HABILITADO` SMALLINT NOT NULL,
  `ID_PERFIL` INT NOT NULL,
  `CORREO_ELECTRONICO` VARCHAR(200) NULL,
  `CLAVE_USUARIO` VARCHAR(256) NULL,
  PRIMARY KEY (`ID_USUARIO`),
  FOREIGN KEY (`ID_PERFIL`) REFERENCES PERFIL(`ID_PERFIL`)
) ENGINE = MyISAM;